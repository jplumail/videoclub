{
  "hosting": {
    "public": "out",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "trailingSlash": false,
    "redirects": [
      {
        "source": "/",
        "destination": "/film/meilleurs",
        "type": 307
      }
    ],
    "headers": [
      {
        "key": "Cache-Control",
        "value": "public, max-age=2592000, immutable"
      }
    ]
  }
}
